#include <iostream>
#include <vector>
#include <string>

int main() {
    // Vetor com modelos de carro (exemplo)
    std::vector<std::string> modelos = {
        "Toyota Corolla",
        "Toyota Hilux",
        "Honda Civic",
        "Honda Fit",
        "Ford Mustang",
        "Ford Fiesta",
        "Chevrolet Onix",
        "Chevrolet Camaro"
    };

    // Exibindo com cout
    std::cout << "Modelos de carro:\n";
    for (const auto& modelo : modelos) {
        std::cout << "- " << modelo << std::endl;
    }

    return 0;
}


#include <iostream>
#include <vector>
using namespace std;

int main() {
    // Vetor com preços de carros (valores fictícios)
    vector<float> precos = {
        145000.0f,  // Toyota Corolla
        280000.0f,  // Toyota Hilux
        160000.0f,  // Honda Civic
        90000.0f,   // Honda Fit
        350000.0f,  // Ford Mustang
        75000.0f,   // Ford Fiesta
        85000.0f,   // Chevrolet Onix
        420000.0f   // Chevrolet Camaro
    };

    cout << "Preços dos carros:\n";
    for (float preco : precos) {
        cout << "R$ " << preco << endl;
    }

    return 0;
}

#include <iostream>
#include <vector>
using namespace std;

int main() {
    // Vetor com 110 notas de alunos (valores fictícios)
    vector<double> notas(110);

    // Preenchendo o vetor com notas (exemplo simples)
    for (int i = 0; i < 110; i++) {
        notas[i] = 5.0 + (i % 6);   // só para gerar notas variadas
    }

    // Exibir o aluno 5 (índice 4, pois começa do zero)
    int aluno = 5;
    cout << "Aluno " << aluno 
         << " tem a nota: " << notas[aluno - 1] << endl;

    return 0;
}



#include <iostream>
 using namespace std;
int main()
{
 for (int i = 1; i <= 10; i++)
{
    cout << i << endl;
}
    return 0;
}

